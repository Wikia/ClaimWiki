{
	"name": "Claim Wiki",
	"version": "2.0",
	"author": [
		"Alexia E. Smith",
		"Curse Inc&copy;"
	],
	"descriptionmsg": "claimwiki_description",
	"type": "specialpage",
	"GroupPermissions": {
		"user": {
			"claim_wiki": true
		},
		"bureaucrat": {
			"wiki_claims": true
		}
	},
	"AvailableRights": [
		"claim_wiki",
		"wiki_claims"
	],
	"SpecialPages": {
		"ClaimWiki": "SpecialClaimWiki",
		"WikiClaims": "SpecialWikiClaims"
	},
	"MessagesDirs": {
		"ClaimWiki": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"SpecialClaimWiki": "ClaimWiki.alias.php"
	},
	"AutoloadClasses": {
		"ClaimWikiHooks": "ClaimWiki.hooks.php",
		"SpecialClaimWiki": "specials/SpecialClaimWiki.php",
		"SpecialWikiClaims": "specials/SpecialWikiClaims.php",
		"WikiClaim": "classes/WikiClaim.php",
		"ClaimLogPager": "classes/ClaimLog.php",
		"ClaimLogEntry": "classes/ClaimLog.php",
		"EchoWikiClaimFormatter": "classes/EchoWikiClaimFormatter.php",
		"EchoWikiClaimPresentationModel": "classes/EchoWikiClaimPresentationModel.php",
		"TemplateClaimEmails": "templates/TemplateClaimEmails.php",
		"TemplateClaimWiki": "templates/TemplateClaimWiki.php",
		"TemplateWikiClaims": "templates/TemplateWikiClaims.php"
	},
	"ResourceModules": {
		"ext.claimWiki": {
			"scripts": [
				"js/listSorter.js"
			],
			"styles": [
				"css/claimwiki.css"
			],
			"dependencies": [
				"ext.hydraCore.pagination",
				"ext.hydraCore.button"
			],
			"position": "top"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteExtPath": "ClaimWiki"
	},
	"callback": "ClaimWikiHooks::onRegistration",
	"Hooks": {
		"BeforeCreateEchoEvent": "ClaimWikiHooks::onBeforeCreateEchoEvent",
		"BeforePageDisplay": "ClaimWikiHooks::onBeforePageDisplay",
		"SkinBuildSidebar": "ClaimWikiHooks::onSkinBuildSidebar",
		"LoadExtensionSchemaUpdates": "ClaimWikiHooks::onLoadExtensionSchemaUpdates",
		"UserEffectiveGroups": "ClaimWikiHooks::onUserEffectiveGroups",
		"UserRights": "ClaimWikiHooks::onUserRights"
	},
	"config": {
		"ClaimWikiEnabled": true,
		"ClaimWikiNumberOfQuestions": 4,
		"ClaimWikiEmailTo": null,
		"ClaimWikiEmailSignature": "The Wiki Team",
		"ClaimWikiGuardianTotal": 1
	},
	"manifest_version": 1
}
